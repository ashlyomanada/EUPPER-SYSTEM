{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  data() {\n    return {\n      UserRatings: [],\n      users: [],\n      selectedUser: null,\n      selectedUserId: \"\",\n      selectedTable: \"ppo_tbl\",\n      formVisible: false,\n      selectedRatings: {\n        id: null,\n        month: \"\",\n        year: \"\",\n        occi: \"\",\n        ormin: \"\",\n        marin: \"\",\n        rom: \"\",\n        pal: \"\",\n        puertop: \"\"\n      }\n    };\n  },\n  created() {\n    this.getUserRatings();\n    this.getUsers();\n  },\n  methods: {\n    async getUserRatings() {\n      try {\n        const response = await axios.get(\"/getUserRatings\");\n        this.UserRatings = response.data;\n      } catch (error) {\n        console.error(\"Error fetching Useratings data:\", error);\n      }\n    },\n    openForm(UserRatings) {\n      this.selectedRatings = {\n        ...UserRatings\n      };\n      this.formVisible = true;\n    },\n    async saveAdmin() {\n      try {\n        const response = await axios.post(\"/api/saveAdmin\", this.selectedAdmin);\n        if (response.status === 200) {\n          const responseData = response.data;\n          if (responseData && responseData.success) {\n            console.log(responseData.message);\n            this.formVisible = false;\n            this.getAdminsInfo();\n          } else {\n            console.error(\"Save failed:\", responseData.message);\n          }\n        } else {\n          console.error(`Unexpected response status: ${response.status}`);\n        }\n      } catch (error) {\n        console.error(\"Error saving admin:\", error);\n      }\n    },\n    async getUsers() {\n      try {\n        const response = await axios.get(\"/getUsers\"); // Replace with your actual endpoint\n        this.users = response.data;\n        console.log(response);\n      } catch (error) {\n        console.error(\"Error fetching users data:\", error);\n      }\n    },\n    closeForm() {\n      this.formVisible = false;\n    },\n    async findData() {\n      try {\n        // Make a request to fetch filtered data based on selected user and table\n        const response = await axios.get(`/findData/${this.selectedUserId}/${this.selectedTable}`);\n        this.UserRatings = response.data;\n      } catch (error) {\n        console.error(\"Error fetching filtered data:\", error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","UserRatings","users","selectedUser","selectedUserId","selectedTable","formVisible","selectedRatings","id","month","year","occi","ormin","marin","rom","pal","puertop","created","getUserRatings","getUsers","methods","response","get","error","console","openForm","saveAdmin","post","selectedAdmin","status","responseData","success","log","message","getAdminsInfo","closeForm","findData"],"sources":["C:\\laragon\\www\\EUPPER-SYSTEM\\src\\components\\Ratings.vue"],"sourcesContent":["<template>\r\n  <div class=\"table-data\">\r\n    <div class=\"order\">\r\n      <div id=\"evaluation-ratings\">\r\n        <div>\r\n          <h3>Unit Performance Evaluation Rating</h3>\r\n          <h4>R1 - DPRM</h4>\r\n        </div>\r\n        <div class=\"table-options\">\r\n          <div class=\"options-control\">\r\n            Select User:\r\n            <select v-model=\"selectedUserId\" class=\"month\" name=\"month\">\r\n              <option\r\n                v-for=\"user in users\"\r\n                :key=\"user.user_id\"\r\n                :value=\"user.user_id\"\r\n              >\r\n                {{ user.username }}\r\n              </option>\r\n            </select>\r\n            Select Table:\r\n            <select v-model=\"selectedTable\" class=\"month\" name=\"month\">\r\n              <option value=\"ppo_tbl\">PPO CPO LEVEL</option>\r\n              <option value=\"2\">RMFB PMFC LEVEL</option>\r\n              <option value=\"3\">MPS CPS LEVEL</option>\r\n            </select>\r\n            <button @click=\"findData\" class=\"find\">\r\n              <i class=\"bx bx-search\"></i>Find\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <table>\r\n        <thead>\r\n          <tr>\r\n            <th>Action</th>\r\n            <th>Office</th>\r\n            <th>Do</th>\r\n            <th>Didm</th>\r\n            <th>Di</th>\r\n            <th>Dpcr</th>\r\n            <th>Dl</th>\r\n            <th>Dhrdd</th>\r\n            <th>Dprm</th>\r\n            <th>Dictm</th>\r\n            <th>Dpl</th>\r\n            <th>Dc</th>\r\n            <th>Drd</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"UserRatings in UserRatings\" :key=\"UserRatings.id\">\r\n            <td class=\"td-btn\">\r\n              <button class=\"pen-btn\" @click=\"openForm(UserRatings)\">\r\n                <i class=\"fa-solid fa-pen fa-lg\"></i>\r\n              </button>\r\n            </td>\r\n            <td>{{ UserRatings.office }}</td>\r\n            <td>{{ UserRatings.do }}</td>\r\n            <td>{{ UserRatings.didm }}.00</td>\r\n            <td>{{ UserRatings.di }}.00</td>\r\n            <td>{{ UserRatings.dpcr }}.00</td>\r\n            <td>{{ UserRatings.dl }}.00</td>\r\n            <td>{{ UserRatings.dhrdd }}.00</td>\r\n            <td>{{ UserRatings.dprm }}.00</td>\r\n            <td>{{ UserRatings.dictm }}</td>\r\n            <td>{{ UserRatings.dpl }}</td>\r\n            <td>{{ UserRatings.dc }}</td>\r\n            <td>{{ UserRatings.drd }}</td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n  </div>\r\n  <form\r\n    class=\"form\"\r\n    id=\"modal-form\"\r\n    :style=\"{ display: formVisible ? 'block' : 'none' }\"\r\n  >\r\n    <input\r\n      v-model=\"selectedRatings.month\"\r\n      type=\"text\"\r\n      placeholder=\"Month\"\r\n      class=\"input\"\r\n    />\r\n    <input\r\n      v-model=\"selectedRatings.year\"\r\n      type=\"text\"\r\n      placeholder=\"Year\"\r\n      class=\"input\"\r\n    />\r\n    <input\r\n      v-model=\"selectedRatings.occi\"\r\n      type=\"text\"\r\n      placeholder=\"Occidental Mindoro\"\r\n      class=\"input\"\r\n    />\r\n    <input\r\n      v-model=\"selectedRatings.ormin\"\r\n      type=\"text\"\r\n      placeholder=\"Oriental Mindoro\"\r\n      class=\"input\"\r\n    />\r\n    <input\r\n      v-model=\"selectedRatings.marin\"\r\n      type=\"text\"\r\n      placeholder=\"Marinduque\"\r\n      class=\"input\"\r\n    />\r\n    <input\r\n      v-model=\"selectedRatings.rom\"\r\n      type=\"text\"\r\n      placeholder=\"Romblon\"\r\n      class=\"input\"\r\n    />\r\n    <input\r\n      v-model=\"selectedRatings.pal\"\r\n      type=\"text\"\r\n      placeholder=\"Palawan\"\r\n      class=\"input\"\r\n    />\r\n    <input\r\n      v-model=\"selectedRatings.puertop\"\r\n      type=\"text\"\r\n      placeholder=\"Puerto Princesa\"\r\n      class=\"input\"\r\n    />\r\n    <div class=\"modal-buttons\">\r\n      <button @click.prevent=\"saveAdmin\">Save</button>\r\n      <button @click.prevent=\"closeForm\">Close</button>\r\n    </div>\r\n  </form>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      UserRatings: [],\r\n      users: [],\r\n      selectedUser: null,\r\n      selectedUserId: \"\",\r\n      selectedTable: \"ppo_tbl\",\r\n      formVisible: false,\r\n      selectedRatings: {\r\n        id: null,\r\n        month: \"\",\r\n        year: \"\",\r\n        occi: \"\",\r\n        ormin: \"\",\r\n        marin: \"\",\r\n        rom: \"\",\r\n        pal: \"\",\r\n        puertop: \"\",\r\n      },\r\n    };\r\n  },\r\n\r\n  created() {\r\n    this.getUserRatings();\r\n    this.getUsers();\r\n  },\r\n\r\n  methods: {\r\n    async getUserRatings() {\r\n      try {\r\n        const response = await axios.get(\"/getUserRatings\");\r\n        this.UserRatings = response.data;\r\n      } catch (error) {\r\n        console.error(\"Error fetching Useratings data:\", error);\r\n      }\r\n    },\r\n\r\n    openForm(UserRatings) {\r\n      this.selectedRatings = { ...UserRatings };\r\n      this.formVisible = true;\r\n    },\r\n\r\n    async saveAdmin() {\r\n      try {\r\n        const response = await axios.post(\"/api/saveAdmin\", this.selectedAdmin);\r\n\r\n        if (response.status === 200) {\r\n          const responseData = response.data;\r\n\r\n          if (responseData && responseData.success) {\r\n            console.log(responseData.message);\r\n            this.formVisible = false;\r\n            this.getAdminsInfo();\r\n          } else {\r\n            console.error(\"Save failed:\", responseData.message);\r\n          }\r\n        } else {\r\n          console.error(`Unexpected response status: ${response.status}`);\r\n        }\r\n      } catch (error) {\r\n        console.error(\"Error saving admin:\", error);\r\n      }\r\n    },\r\n\r\n    async getUsers() {\r\n      try {\r\n        const response = await axios.get(\"/getUsers\"); // Replace with your actual endpoint\r\n        this.users = response.data;\r\n        console.log(response);\r\n      } catch (error) {\r\n        console.error(\"Error fetching users data:\", error);\r\n      }\r\n    },\r\n\r\n    closeForm() {\r\n      this.formVisible = false;\r\n    },\r\n\r\n    async findData() {\r\n      try {\r\n        // Make a request to fetch filtered data based on selected user and table\r\n        const response = await axios.get(\r\n          `/findData/${this.selectedUserId}/${this.selectedTable}`\r\n        );\r\n        this.UserRatings = response.data;\r\n      } catch (error) {\r\n        console.error(\"Error fetching filtered data:\", error);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.evaluation-ratings {\r\n  display: flex;\r\n  align-items: center;\r\n  grid-gap: 16px;\r\n  margin-bottom: 24px;\r\n  flex-direction: column;\r\n}\r\n.table-options {\r\n  width: 100%;\r\n  display: flex;\r\n  justify-content: flex-end;\r\n  align-items: center;\r\n  padding: 1rem;\r\n}\r\n.options-control {\r\n  display: flex;\r\n  gap: 1rem;\r\n}\r\n</style>\r\n"],"mappings":"AAuIA,OAAOA,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,KAAK,EAAE,EAAE;MACTC,YAAY,EAAE,IAAI;MAClBC,cAAc,EAAE,EAAE;MAClBC,aAAa,EAAE,SAAS;MACxBC,WAAW,EAAE,KAAK;MAClBC,eAAe,EAAE;QACfC,EAAE,EAAE,IAAI;QACRC,KAAK,EAAE,EAAE;QACTC,IAAI,EAAE,EAAE;QACRC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE,EAAE;QACTC,KAAK,EAAE,EAAE;QACTC,GAAG,EAAE,EAAE;QACPC,GAAG,EAAE,EAAE;QACPC,OAAO,EAAE;MACX;IACF,CAAC;EACH,CAAC;EAEDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,cAAc,CAAC,CAAC;IACrB,IAAI,CAACC,QAAQ,CAAC,CAAC;EACjB,CAAC;EAEDC,OAAO,EAAE;IACP,MAAMF,cAAcA,CAAA,EAAG;MACrB,IAAI;QACF,MAAMG,QAAO,GAAI,MAAMtB,KAAK,CAACuB,GAAG,CAAC,iBAAiB,CAAC;QACnD,IAAI,CAACrB,WAAU,GAAIoB,QAAQ,CAACrB,IAAI;MAClC,EAAE,OAAOuB,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,iCAAiC,EAAEA,KAAK,CAAC;MACzD;IACF,CAAC;IAEDE,QAAQA,CAACxB,WAAW,EAAE;MACpB,IAAI,CAACM,eAAc,GAAI;QAAE,GAAGN;MAAY,CAAC;MACzC,IAAI,CAACK,WAAU,GAAI,IAAI;IACzB,CAAC;IAED,MAAMoB,SAASA,CAAA,EAAG;MAChB,IAAI;QACF,MAAML,QAAO,GAAI,MAAMtB,KAAK,CAAC4B,IAAI,CAAC,gBAAgB,EAAE,IAAI,CAACC,aAAa,CAAC;QAEvE,IAAIP,QAAQ,CAACQ,MAAK,KAAM,GAAG,EAAE;UAC3B,MAAMC,YAAW,GAAIT,QAAQ,CAACrB,IAAI;UAElC,IAAI8B,YAAW,IAAKA,YAAY,CAACC,OAAO,EAAE;YACxCP,OAAO,CAACQ,GAAG,CAACF,YAAY,CAACG,OAAO,CAAC;YACjC,IAAI,CAAC3B,WAAU,GAAI,KAAK;YACxB,IAAI,CAAC4B,aAAa,CAAC,CAAC;UACtB,OAAO;YACLV,OAAO,CAACD,KAAK,CAAC,cAAc,EAAEO,YAAY,CAACG,OAAO,CAAC;UACrD;QACF,OAAO;UACLT,OAAO,CAACD,KAAK,CAAE,+BAA8BF,QAAQ,CAACQ,MAAO,EAAC,CAAC;QACjE;MACF,EAAE,OAAON,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,qBAAqB,EAAEA,KAAK,CAAC;MAC7C;IACF,CAAC;IAED,MAAMJ,QAAQA,CAAA,EAAG;MACf,IAAI;QACF,MAAME,QAAO,GAAI,MAAMtB,KAAK,CAACuB,GAAG,CAAC,WAAW,CAAC,EAAE;QAC/C,IAAI,CAACpB,KAAI,GAAImB,QAAQ,CAACrB,IAAI;QAC1BwB,OAAO,CAACQ,GAAG,CAACX,QAAQ,CAAC;MACvB,EAAE,OAAOE,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,4BAA4B,EAAEA,KAAK,CAAC;MACpD;IACF,CAAC;IAEDY,SAASA,CAAA,EAAG;MACV,IAAI,CAAC7B,WAAU,GAAI,KAAK;IAC1