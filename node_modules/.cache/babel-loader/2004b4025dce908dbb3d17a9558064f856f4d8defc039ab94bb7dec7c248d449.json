le","log","catch","error"],"sources":["C:\\laragon\\www\\EUPPER-SYSTEM\\src\\components\\UserPPO.vue"],"sourcesContent":["<template>\r\n  <div class=\"table-data\">\r\n    <div class=\"order\">\r\n      <div class=\"head\">\r\n        <div>\r\n          <h3>Unit Performance Evaluation Rating</h3>\r\n          <h4>Oriental Mindoro PPO / CPO Level</h4>\r\n        </div>\r\n      </div>\r\n      <table>\r\n        <thead>\r\n          <tr>\r\n            <th>Month</th>\r\n            <th>Year</th>\r\n            <th>Office</th>\r\n            <th>DO</th>\r\n            <th>DIDM</th>\r\n            <th>DI</th>\r\n            <th>DPCR</th>\r\n            <th>Dl</th>\r\n            <th>Dhrdd</th>\r\n            <th>Dprm</th>\r\n            <th>Dictm</th>\r\n            <th>Dpl</th>\r\n            <th>Dc</th>\r\n            <th>Drd</th>\r\n          </tr>\r\n          <tr v-if=\"dataFetched\" v-for=\"rating in userRatings\" :key=\"rating.id\">\r\n            <td>{{ month }}</td>\r\n            <td>{{ year }}</td>\r\n            <td>{{ office }}</td>\r\n            <td>{{ Do }}</td>\r\n            <td>{{ didm }}</td>\r\n            <td>{{ di }}</td>\r\n            <td>{{ dpcr }}</td>\r\n            <td>{{ dl }}</td>\r\n            <td>{{ dhrdd }}</td>\r\n            <td>{{ dprm }}</td>\r\n            <td>{{ dictm }}</td>\r\n            <td>{{ dpl }}</td>\r\n            <td>{{ dc }}</td>\r\n            <td>{{ drd }}</td>\r\n          </tr>\r\n          <tr v-else>\r\n            <h2>No Ratings Yet</h2>\r\n          </tr>\r\n        </thead>\r\n      </table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      dataFetched: false,\r\n      month: \"\", // Use lowercase for property names\r\n      year: \"\",\r\n      office: \"\",\r\n      Do: \"\",\r\n      didm: \"\",\r\n      di: \"\",\r\n      dpcr: \"\",\r\n      dl: \"\",\r\n      dhrdd: \"\",\r\n      dprm: \"\",\r\n      dictm: \"\",\r\n      dpl: \"\",\r\n      dc: \"\",\r\n      drd: \"\",\r\n    };\r\n  },\r\n\r\n  mounted() {\r\n    this.fetchData();\r\n  },\r\n\r\n  methods: {\r\n    fetchData() {\r\n      const storedUserId = sessionStorage.getItem(\"id\");\r\n\r\n      if (storedUserId) {\r\n        axios\r\n          .get(`/viewUserRatings/${storedUserId}`)\r\n          .then((response) => {\r\n            const userRatings = response.data;\r\n\r\n            if (userRatings.length > 0) {\r\n              // Take the first item in the array\r\n              const firstRating = userRatings[0];\r\n\r\n              this.month = firstRating.month;\r\n              this.year = firstRating.year;\r\n              this.office = firstRating.office;\r\n              this.Do = firstRating.do;\r\n              this.didm = firstRating.didm;\r\n              this.di = firstRating.di;\r\n              this.dpcr = firstRating.dpcr;\r\n              this.dl = firstRating.dl;\r\n              this.dhrdd = firstRating.dhrdd;\r\n              this.dprm = firstRating.dprm;\r\n              this.dictm = firstRating.dictm;\r\n              this.dpl = firstRating.dpl;\r\n              this.dc = firstRating.dc;\r\n              this.drd = firstRating.drd;\r\n\r\n              this.dataFetched = true;\r\n            } else {\r\n              console.log(\"No ratings found.\");\r\n            }\r\n          })\r\n          .catch((error) => {\r\n            console.error(\"Error fetching user data:\", error);\r\n          });\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style></style>\r\n"],"mappings":"AAqDA,OAAOA,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,KAAK;MAClBC,KAAK,EAAE,EAAE;MAAE;MACXC,IAAI,EAAE,EAAE;MACRC,MAAM,EAAE,EAAE;MACVC,EAAE,EAAE,EAAE;MACNC,IAAI,EAAE,EAAE;MACRC,EAAE,EAAE,EAAE;MACNC,IAAI,EAAE,EAAE;MACRC,EAAE,EAAE,EAAE;MACNC,KAAK,EAAE,EAAE;MACTC,IAAI,EAAE,EAAE;MACRC,KAAK,EAAE,EAAE;MACTC,GAAG,EAAE,EAAE;MACPC,EAAE,EAAE,EAAE;MACNC,GAAG,EAAE;IACP,CAAC;E