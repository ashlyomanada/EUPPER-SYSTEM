his.loginError = \"Email and password are required\";\r\n        return;\r\n      }\r\n\r\n      this.isLoading = true;\r\n\r\n      axios\r\n        .post(\"login\", loginData)\r\n        .then((response) => {\r\n          if (response.data.success) {\r\n            this.$router.replace(\"/home\");\r\n          } else {\r\n            this.loginError = \"Invalid email or password\";\r\n            console.error(\"Login failed:\", response.data.message);\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          this.loginError = \"An error occurred during login. Please try again.\";\r\n          console.error(\"Login error:\", error);\r\n          console.log(\"Login Data:\", loginData);\r\n          console.error(\"Login failed:\", response.data.message);\r\n          // ...\r\n\r\n          // Log additional details if available\r\n          if (error.response) {\r\n            console.error(\"Response status:\", error.response.status);\r\n            console.error(\"Response data:\", error.response.data);\r\n          } else if (error.request) {\r\n            console.error(\r\n              \"No response received. Request details:\",\r\n              error.request\r\n            );\r\n          } else {\r\n            console.error(\"Error details:\", error.message);\r\n          }\r\n        })\r\n        .finally(() => {\r\n          this.isLoading = false;\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.login-title {\r\n  white-space: nowrap;\r\n}\r\n.title-name {\r\n  text-align: center;\r\n}\r\n\r\n.error-message {\r\n  color: red;\r\n  margin-top: 10px;\r\n}\r\n</style>\r\n"],"mappings":"AAoCA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,UAAU,EAAE,IAAI;MAChBC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,KAAKA,CAAA,EAAG;MACN,MAAMC,SAAQ,GAAI;QAChBN,KAAK,EAAE,IAAI,CAACA,KAAK;QACjBC,QAAQ,EAAE,IAAI,CAACA;MACjB,CAAC;;MAED;MACAM,OAAO,CAACC,GAAG,CAAC,aAA