Data.success) {\r\n            console.log(responseData.message);\r\n            this.formVisible = false;\r\n            this.getAdminsInfo();\r\n          } else {\r\n            console.error(\"Save failed:\", responseData.message);\r\n          }\r\n        } else {\r\n          console.error(`Unexpected response status: ${response.status}`);\r\n        }\r\n      } catch (error) {\r\n        console.error(\"Error saving admin:\", error);\r\n      }\r\n    },\r\n\r\n    openForm(UserRatings) {\r\n      this.selectedRatings = { ...UserRatings };\r\n      this.formVisible = true;\r\n    },\r\n\r\n    closeForm() {\r\n      this.formVisible = false;\r\n    },\r\n\r\n    async findData() {\r\n      try {\r\n        // Make a request to fetch filtered data based on selected user and table\r\n        const response = await axios.get(\r\n          `/findData/${this.selectedUserId}/${this.selectedTable}`\r\n        );\r\n        this.UserRatings = response.data;\r\n      } catch (error) {\r\n        console.error(\"Error fetching filtered data:\", error);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n#rating-form-edit {\r\n  position: absolute;\r\n  width: 60%;\r\n  top: 15%;\r\n  left: 20%;\r\n  display: none;\r\n}\r\n.form-edit {\r\n  display: flex;\r\n  gap: 1rem;\r\n}\r\n.evaluation-ratings {\r\n  display: flex;\r\n  align-items: center;\r\n  grid-gap: 16px;\r\n  margin-bottom: 24px;\r\n  flex-direction: column;\r\n}\r\n.table-options {\r\n  width: 100%;\r\n  display: flex;\r\n  justify-content: flex-end;\r\n  align-items: center;\r\n  padding: 1rem;\r\n}\r\n.options-control {\r\n  display: flex;\r\n  gap: 1rem;\r\n}\r\n</style>\r\n"],"mappings":"AAmLA,OAAOA,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,KAAK,EAAE,EAAE;MACTC,YAAY,EAAE,IAAI;MAClBC,cAAc,EAAE,EAAE;MAClBC,aAAa,EAAE,CAAC;MAChBC,WAAW,EAAE,KAAK;MAClBC,eAAe,EAAE;QACfC,EAAE,EAAE,IAAI;QACRC,MAAM,EAAE,EAAE;QACVC,EAAE,EAAE,EAAE;QACNC,IAAI,EAAE,EAAE;QACRC