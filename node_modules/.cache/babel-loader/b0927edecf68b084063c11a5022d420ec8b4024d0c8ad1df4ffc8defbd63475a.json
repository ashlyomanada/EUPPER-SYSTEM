{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  data() {\n    return {\n      AdminsInfo: [],\n      formVisible: false,\n      selectedAdmin: {\n        admin_id: null,\n        username: \"\",\n        password: \"\",\n        email: \"\",\n        phone_no: \"\"\n      }\n    };\n  },\n  created() {\n    this.getAdminsInfo();\n  },\n  methods: {\n    async getAdminsInfo() {\n      try {\n        const response = await axios.get(\"/api/getAdmins\"); // Update the endpoint accordingly\n        this.AdminsInfo = response.data;\n      } catch (error) {\n        console.error(\"Error fetching admin data:\", error);\n      }\n    },\n    async updateAdmin() {\n      try {\n        const response = await axios.post(\"/api/updateAdmin\", this.selectedAdmin); // Update the endpoint accordingly\n        console.log(\"Full Response (updateAdmin):\", response);\n        if (response.status === 200) {\n          const responseData = response.data;\n          if (responseData && responseData.message) {\n            console.log(responseData.message);\n            this.formVisible = false;\n            this.getAdminsInfo();\n          } else if (responseData && responseData.error) {\n            console.error(responseData.error);\n          } else {\n            console.error(\"Unexpected response format\");\n          }\n        } else {\n          console.error(`Unexpected response status: ${response.status}`);\n        }\n      } catch (error) {\n        console.error(\"Error updating admin:\", error);\n      }\n    },\n    closeForm() {\n      this.formVisible = false;\n    },\n    openForm(adminInfo) {\n      this.selectedAdmin = {\n        ...adminInfo\n      };\n      this.formVisible = true;\n    }\n  }\n};","map":{"version":3,"names":["axios","data","AdminsInfo","formVisible","selectedAdmin","admin_id","username","password","email","phone_no","created","getAdminsInfo","methods","response","get","error","console","updateAdmin","post","log","status",