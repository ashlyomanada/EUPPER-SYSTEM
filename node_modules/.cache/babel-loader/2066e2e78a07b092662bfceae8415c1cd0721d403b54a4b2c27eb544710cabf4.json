s.getUsersInfo();\n          } else {\n            console.error(\"Save failed:\", responseData.message);\n          }\n        } else {\n          console.error(`Unexpected response status: ${response.status}`);\n        }\n      } catch (error) {\n        console.error(\"Error saving admin:\", error);\n      }\n    },\n    closeForm() {\n      this.formVisible = false;\n    }\n  }\n};","map":{"version":3,"names":["axios","data","UsersInfo","dataFetched","userRatings","formVisible","selectedRatings","id","office","do","didm","di","dpcr","dl","dhrdd","dprm","dictm","dpl","dc","drd","mounted","fetchData","getUsersInfo","methods","get","e","console","error","storedUserId","sessionStorage","getItem","then","response","catch","openForm","UserRatings","saveUserRates","post","status","responseData","success","message","closeForm"],"sources":["C:\\laragon\\www\\EUPPER-SYSTEM\\src\\components\\UserPPO.vue"],"sourcesContent":["<template>\r\n  <div class=\"table-data\">\r\n    <div class=\"order\">\r\n      <div class=\"head\">\r\n        <div>\r\n          <h3>Unit Performance Evaluation Rating</h3>\r\n          <h4>Oriental Mindoro PPO / CPO Level</h4>\r\n        </div>\r\n      </div>\r\n      <table v-if=\"dataFetched\">\r\n        <thead>\r\n          <tr>\r\n            <th>Action</th>\r\n            <th>Month</th>\r\n            <th>Year</th>\r\n            <th>Office</th>\r\n            <th>DO</th>\r\n            <th>DIDM</th>\r\n            <th>DI</th>\r\n            <th>DPCR</th>\r\n            <th>Dl</th>\r\n            <th>Dhrdd</th>\r\n            <th>Dprm</th>\r\n            <th>Dictm</th>\r\n            <th>Dpl</th>\r\n            <th>Dc</th>\r\n            <th>Drd</th>\r\n          </tr>\r\n          <tr v-for=\"rating in userRatings\" :key=\"rating.id\">\r\n            <td class=\"td-btn\">\r\n              <button\r\n                class=\"pen-btn\"\r\n                @click=\"openForm(rating)\"\r\n                v-if=\"UsersInfo.status === 'Enable'\"\r\n              >\r\n                <i class=\"fa-solid fa-pen fa-lg\"></i>\r\n              </button>\r\n            </td>\r\n            <td>{{ rating.month }}</td>\r\n            <td>{{ rating.year }}</td>\r\n            <td>{{ rating.office }}</td>\r\n            <td>{{ rating.do }}</td>\r\n            <td>{{ rating.didm }}</td>\r\n            <td>{{ rating.di }}</td>\r\n            <td>{{ rating.dpcr }}</td>\r\n            <td>{{ rating.dl }}</td>\r\n            <td>{{ rating.dhrdd }}</td>\r\n            <td>{{ rating.dprm }}</td>\r\n            <td>{{ rating.dictm }}</td>\r\n            <td>{{ rating.dpl }}</td>\r\n            <td>{{ rating.dc }}</td>\r\n            <td>{{ rating.drd }}</td>\r\n          </tr>\r\n        </thead>\r\n      </table>\r\n      <h2 v-else>No Ratings Yet</h2>\r\n    </div>\r\n  </div>\r\n  <form\r\n    class=\"form\"\r\n    id=\"rating-form-edit2\"\r\n    :style=\"{ display: formVisible ? 'block' : 'none' }\"\r\n  >\r\n    <div class=\"form-edit\">\r\n      <label for=\"\">Office:</label>\r\n      <input\r\n        v-model=\"selectedRatings.office\"\r\n        type=\"text\"\r\n        placeholder=\"\"\r\n        class=\"input\"\r\n      />\r\n      <label for=\"\">Do:</label>\r\n      <input\r\n        v-model=\"selectedRatings.do\"\r\n        type=\"text\"\r\n        placeholder=\"\"\r\n        class=\"input\"\r\n      />\r\n      <label for=\"\">Didm:</label>\r\n      <input\r\n        v-model=\"selectedRatings.didm\"\r\n        type=\"text\"\r\n        placeholder=\" Mindoro\"\r\n        class=\"input\"\r\n      />\r\n    </div>\r\n    <div class=\"form-edit\">\r\n      <label for=\"\">Di:</label>\r\n      <input\r\n        v-model=\"selectedRatings.di\"\r\n        type=\"text\"\r\n        placeholder=\" Mindoro\"\r\n        class=\"input\"\r\n      />\r\n      <label for=\"\">Dpcr:</label>\r\n      <input\r\n        v-model=\"selectedRatings.dpcr\"\r\n        type=\"text\"\r\n        placeholder=\"\"\r\n        class=\"input\"\r\n      />\r\n      <label for=\"\">Dl:</label>\r\n      <input\r\n        v-model=\"selectedRatings.dl\"\r\n        type=\"text\"\r\n        placeholder=\"\"\r\n        class=\"input\"\r\n      />\r\n    </div>\r\n    <div class=\"form-edit\">\r\n      <label for=\"\">Dhrdd:</label>\r\n      <input\r\n        v-model=\"selectedRatings.dhrdd\"\r\n        type=\"text\"\r\n        placeholder=\"\"\r\n        class=\"input\"\r\n      />\r\n      <label for=\"\">Dprm:</label>\r\n      <input\r\n        v-model=\"selectedRatings.dprm\"\r\n        type=\"text\"\r\n        placeholder=\"\"\r\n        class=\"input\"\r\n      />\r\n      <label for=\"\">Dictm:</label>\r\n      <input\r\n        v-model=\"selectedRatings.dictm\"\r\n        type=\"text\"\r\n        placeholder=\"\"\r\n        class=\"input\"\r\n      />\r\n    </div>\r\n    <div class=\"form-edit\">\r\n      <label for=\"\">Dpl:</label>\r\n      <input\r\n        v-model=\"selectedRatings.dpl\"\r\n        type=\"text\"\r\n        placeholder=\"\"\r\n        class=\"input\"\r\n      />\r\n      <label for=\"\">Dc:</label>\r\n      <input\r\n        v-model=\"selectedRatings.dc\"\r\n        type=\"text\"\r\n        placeholder=\"\"\r\n        class=\"input\"\r\n      />\r\n      <label for=\"\">Drd:</label>\r\n      <input\r\n        v-model=\"selectedRatings.drd\"\r\n        type=\"text\"\r\n        placeholder=\"\"\r\n        class=\"input\"\r\n      />\r\n    </div>\r\n    <div class=\"modal-buttons\">\r\n      <button @click.prevent=\"saveUserRates\">Save</button>\r\n      <button @click.prevent=\"closeForm\">Close</button>\r\n    </div>\r\n  </form>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      UsersInfo: [],\r\n      dataFetched: false,\r\n      userRatings: [],\r\n      formVisible: false,\r\n      selectedRatings: {\r\n        id: null,\r\n        office: \"\",\r\n        do: \"\",\r\n        didm: \"\",\r\n        di: \"\",\r\n  